.search{
  background-color: #edece8;
  padding: 20rpx;
  min-height: 100vh;
}

.search_warp{
  display: flex;
  height: 70rpx;
}
.search_warp input{
  flex: 9;
  background-color: #FFF;
  height: 100%;
  padding-left: 20rpx;
}

.search_warp button{
  padding: 0 20rpx;
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #666;
  min-height: 100%;
  margin-left: 10rpx;
}

.search_val_warp{
  margin-top: 20rpx;
  background-color: #eeeeee;
}

.search_val{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 36rpx;
  border: 1px #d6d6d6 solid;
  padding: 15rpx;
  color: #2c2c2c;
}